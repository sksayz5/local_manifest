<?xml version="1.0" encoding="UTF-8"?>

<manifest>

<!-- fetching sources -->      

        <remote name="clo"
                fetch="https://github.com/sksayz5"
                revision="aospa" />

        <remote  name="blobs-gitlab"
                 fetch="https://gitlab.com/ThankYouMario"
                 revision="uvite" />

    
<!-- device trees -->

        <project path="device/realme/RMX1901" name="device_realme_RMX1901_udc" remote="clo" />

<!-- Replacement -->
        <remove-project name="AOSPA/android_build" />
        <remove-project name="AOSPA/android_frameworks_opt_telephony" />
        <remove-project name="AOSPA/android_vendor_aospa" />
        <remove-project name="android_vendor_aospa_prebuilt" />
        <remove-project name="AOSPA/android_frameworks_base" />
        <project path="frameworks/base" name="frameworks_base" remote="clo" />
        <project path="frameworks/opt/telephony" name="frameworks_opt_telephony" remote="clo" />
        <project path="vendor/aospa/prebuilt" name="android_vendor_aospa_prebuilt" remote="blobs-gitlab" />
        <project path="vendor/aospa" name="android_vendor_aospa" remote="clo" >
        <linkfile src="build.sh" dest="rom-build.sh" />
        </project>
        <project path="build/make" name="build_make" remote="clo" >
        <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
        <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
        <linkfile src="core" dest="build/core" />
        <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
        <linkfile src="target" dest="build/target" />
        <linkfile src="tools" dest="build/tools" />
        </project>
 </manifest>
